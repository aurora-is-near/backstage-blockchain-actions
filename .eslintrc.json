{
  "plugins": ["jest", "@typescript-eslint"],
  "extends": ["plugin:github/recommended"],
  "parser": "@typescript-eslint/parser",
  "rules": {
    "import/no-namespace": "off",
    "camelcase": "off",
    "i18n-text/no-en": "off"
  },
  "env": {
    "node": true,
    "es6": true,
    "jest/globals": true
  }
}
